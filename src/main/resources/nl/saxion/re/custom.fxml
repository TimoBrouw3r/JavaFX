<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.saxion.re.CustomController">

    <HBox alignment="CENTER" fx:id="picturePane">
    
    </HBox>    
    <HBox alignment="CENTER"> 
    <Button fx:id="prev" text="prev" onAction="#prev"/> 
    <Region HBox.hgrow="ALWAYS"/> 
    <Button fx:id="next" text="next" onAction="#next"/> 
    </HBox>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
